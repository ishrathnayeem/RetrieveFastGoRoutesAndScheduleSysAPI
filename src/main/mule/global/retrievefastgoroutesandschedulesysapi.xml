                <http:headers>#[vars.outboundHeaders default {}]</http:headers>
            </http:response>
            <http:error-response statusCode="#[vars.httpStatus default 500]">
                <http:body>#[payload]</http:body>
                <http:headers>#[vars.outboundHeaders default {}]</http:headers>
            </http:error-response>
        </http:listener>
        <apikit:console config-ref="retrievefastgoroutesandschedulesysapi-config" />
		<error-handler ref="fastGoglobalErrorHandlerError_Handler" />
    </flow>
	<sub-flow name="retrievefastgoroutesandschedulesysapiSub_Flow" doc:id="f9da95eb-4724-4972-b7eb-358f3f1e4c65" >
		<ee:transform doc:name="Transform Message" doc:id="9344c3e5-a543-4249-b427-9a43410602fd" >
			<ee:variables >
				<ee:set-variable variableName="transportType" ><![CDATA[attributes.uriParams.'transportType']]></ee:set-variable>
				<ee:set-variable variableName="transportCompany" ><![CDATA[attributes.uriParams.'transportCompany']]></ee:set-variable>
				<ee:set-variable variableName="transactionId" ><![CDATA[%dw 2.0
output application/java
---
attributes.headers.transactionId]]></ee:set-variable>
			</ee:variables>
		</ee:transform>
	</sub-flow>
	<flow name="get:\(transportType)\(transportCompany)\routes:retrievefastgoroutesandschedulesysapi-config">
		<flow-ref doc:name="Flow Reference Initialise Request Variable" doc:id="52596314-f382-4db4-a142-62dea4f0792e" name="retrievefastgoroutesandschedulesysapiSub_Flow"/>
