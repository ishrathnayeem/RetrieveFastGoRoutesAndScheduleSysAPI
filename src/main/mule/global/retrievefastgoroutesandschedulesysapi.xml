                <http:headers>#[vars.outboundHeaders default {}]</http:headers>
            </http:response>
            <http:error-response statusCode="#[vars.httpStatus default 500]">
                <http:body>#[payload]</http:body>
                <http:headers>#[vars.outboundHeaders default {}]</http:headers>
            </http:error-response>
        </http:listener>
        <apikit:router config-ref="retrievefastgoroutesandschedulesysapi-config" />
		<error-handler ref="fastGoglobalErrorHandlerError_Handler" />
    </flow>
    <flow name="retrievefastgoroutesandschedulesysapi-console">
        <http:listener config-ref="retrievefastgoroutesandschedulesysapi-httpListenerConfig" path="/console/*">
